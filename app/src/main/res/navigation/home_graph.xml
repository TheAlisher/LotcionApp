<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_home_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.alis.lotcion.ui.fragments.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_homeFragment_to_lot_graph"
            app:destination="@id/lot_graph">
            <argument
                android:name="lotID"
                android:defaultValue="lot 0"
                app:argType="string" />
        </action>
    </fragment>

    <include app:graph="@navigation/lot_graph" />


</navigation>