<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AuthenticationInputLayout" parent="Widget.MaterialComponents.TextInputLayout.FilledBox">
        <item name="android:layout_margin">10dp</item>
        <item name="boxStrokeWidth">0dp</item>
        <item name="hintEnabled">false</item>
    </style>

    <style name="ProfileButton" parent="Widget.MaterialComponents.Button.OutlinedButton">
        <item name="android:layout_margin">10dp</item>
        <item name="cornerRadius">100dp</item>
        <item name="iconGravity">textEnd</item>
        <item name="iconSize">33dp</item>
        <item name="iconTint">@color/Black</item>
    </style>

    <style name="AddLotInputLayout" parent="Widget.MaterialComponents.TextInputLayout.FilledBox">
        <item name="android:layout_margin">10dp</item>
        <item name="android:elevation">10dp</item>
        <item name="boxBackgroundColor">@color/white</item>
        <item name="boxStrokeWidth">1dp</item>
        <item name="hintEnabled">false</item>
    </style>


</resources>